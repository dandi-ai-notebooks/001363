def calculate_band_power_changes(f_pre, Pxx_pre, f_post, Pxx_post, bands):
    changes = []
    
    for band_name, (low_freq, high_freq) in bands.items():
        pre_power = get_band_power(f_pre, Pxx_pre, low_freq, high_freq)
